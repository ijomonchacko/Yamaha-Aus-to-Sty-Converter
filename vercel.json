{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python@3.9"
    }
  },
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/index.py"
    }
  ],
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "src/**/*", "use": "@vercel/static" }
  ]
}
